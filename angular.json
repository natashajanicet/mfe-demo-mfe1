{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "auth-mfe": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/auth-mfe",
      "sourceRoot": "projects/auth-mfe/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/auth-mfe",
            "index": "projects/auth-mfe/src/index.html",
            "main": "projects/auth-mfe/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/auth-mfe/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/auth-mfe/src/favicon.ico",
              "projects/auth-mfe/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/auth-mfe/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules"
              ]
            },
            "scripts": [],
            "extraWebpackConfig": "projects/auth-mfe/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/auth-mfe/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "auth-mfe:build:production",
              "extraWebpackConfig": "projects/auth-mfe/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "auth-mfe:build:development",
              "proxyConfig": "projects/auth-mfe/src/proxy.conf.json"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200,
            "publicHost": "http://localhost:4200",
            "extraWebpackConfig": "projects/auth-mfe/webpack.config.js"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "auth-mfe:build",
            "extraWebpackConfig": "projects/auth-mfe/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/auth-mfe/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/auth-mfe/src/favicon.ico",
              "projects/auth-mfe/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/purple-green.css",
              "projects/auth-mfe/src/styles.scss"
            ],
            "scripts": [],
            "karmaConfig": "projects/auth-mfe/karma.conf.js"
          }
        }
      }
    }
  }
}
